	<queryString>
		<![CDATA[SELECT
     buyer.*,
     bill.*,
     bill.`bill_id` AS bill_bill_id,
     bill.`cust_id` AS bill_cust_id,
     bill.`date` AS bill_date,
     bill_details.`det_id` AS bill_details_det_id,
     bill_details.`det_bill_id` AS bill_details_det_bill_id,
     bill_details.`barcode_id` AS bill_details_barcode_id,
     bill_details.`price` AS bill_details_price,
     bill_details.`qty` AS bill_details_qty,
     bill_details.`total` AS bill_details_total,
     buyer.`buyer_id` AS buyer_buyer_id,
     buyer.`name` AS buyer_name,
     buyer.`contact` AS buyer_contact,
     buyer.`email` AS buyer_email,
     buyer.`address` AS buyer_address,
     buyer.`gender` AS buyer_gender,
     bill.`bill_id` AS bill_bill_id,
     bill.`cust_id` AS bill_cust_id,
     bill.`date` AS bill_date,
     bill_details.`det_id` AS bill_details_det_id,
     bill_details.`det_bill_id` AS bill_details_det_bill_id,
     bill_details.`barcode_id` AS bill_details_barcode_id,
     bill_details.`price` AS bill_details_price,
     bill_details.`qty` AS bill_details_qty,
     bill_details.`total` AS bill_details_total,
     product.`pro_id` AS product_pro_id,
     product.`product` AS product_product,
     product.`category` AS product_category,
     product.`brand` AS product_brand,
     product.`costprice` AS product_costprice,
     product.`retailprice` AS product_retailprice,
     product.`qty` AS product_qty,
     product.`barcode` AS product_barcode,
     product.`status` AS product_status,
     buyer.`buyer_id` AS buyer_buyer_id,
     buyer.`name` AS buyer_name,
     buyer.`contact` AS buyer_contact,
     buyer.`email` AS buyer_email,
     buyer.`address` AS buyer_address,
     buyer.`gender` AS buyer_gender
FROM
     `bill` bill inner join
     `bill_details` bill_details inner join
     `buyer` buyer inner join
     `product` product where  bill.`bill_id`= bill_details.`det_bill_id` and  bill_details.`barcode_id`=  product.`barcode` and  bill.`cust_id`= buyer.`buyer_id` and bill.`bill_id`=$P{invoice_no};]]>
	</queryString>
